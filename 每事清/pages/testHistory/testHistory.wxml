<view class="page">

  <view class='item' wx:if="{{hadHistory==true}}" wx:for="{{historyQuestionBank}}" bindtap='showDetail' data-index="{{index}}">
    <view class='itemName'>{{item.choseQuestionBank}}</view>
    <view class='peopleNumber'>99+人做过</view>
  </view>

  <view wx:if="{{hadHistory==false}}">
    <image class='noHistoryImage' src='../../images/noHistoryImage.png'></image>
  </view>

</view>